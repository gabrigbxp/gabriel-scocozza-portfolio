{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "composite": true,
    "types": ["cypress", "@testing-library/cypress"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}
